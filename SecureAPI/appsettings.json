{
  "AzureAd": {
    "Instance": "https://{Your Azure B2C Domain Name}.b2clogin.com/",
    "Domain": "{Your Azure B2C Domain Name}.onmicrosoft.com",
    "TenantId": "{Your Azure B2C Domain TenantId}",
    "ClientId": "{Your Azure B2C Domain ClientId}",
    "CallbackPath": "/signin-oidc",
    "Scopes": "access_as_user",
    "SignUpSignInPolicyId": "B2C_1_FrontEndSignUpSignIn",
    "SignedOutCallbackPath": "/signout/B2C_1_susi",
    "ResetPasswordPolicyId": "b2c_1_reset",
    "EditProfilePolicyId": "b2c_1_edit_profile",
    "EnablePiiLogging": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}